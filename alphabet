#include <stdio.h>
int main() 
{
	printf("reg.no:192211541");
    char c;
    printf("\nEnter a character: ");
    scanf("%c", &c);

    if ((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z'))
        printf("%c is an alphabet.", c);
    else
        printf("%c is not an alphabet.", c);

    return 0;
}
