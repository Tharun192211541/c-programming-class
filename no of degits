#include <stdio.h>  
int main()  
{   
   printf("reg.no:192211541");
   int n;  
   int count=0;   
   printf("\nEnter a number");  
   scanf("%d",&n);  
   while(n!=0)  
   {  
       n=n/10;  
       count++;  
   }  
     
   printf("\nThe number of digits in an integer is : %d",count);  
    return 0;  
}  
